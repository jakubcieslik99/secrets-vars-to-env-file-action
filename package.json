{
  "name": "secrets-vars-to-env-file-action",
  "version": "1.5.0",
  "description": "Export GitHub secrets & vars to environment variables & specified file",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest --testPathIgnorePatterns test/e2e --watchAll",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest --testPathPatterns='test/e2e/.*\\.test\\.ts'",
    "build": "tsc",
    "package": "ncc build --source-map --license licenses.txt",
    "prepare:release": "rimraf build && pnpm run build && pnpm run package",
    "lint": "eslint .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts)\"",
    "update:all": "corepack up && pnpm update"
  },
  "author": "Jakub Cie≈õlik",
  "license": "MIT",
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
  "dependencies": {
    "@actions/core": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@jest/globals": "^30.2.0",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
