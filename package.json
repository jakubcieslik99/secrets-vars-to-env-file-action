{
  "name": "secrets-vars-to-env-file-action",
  "version": "1.4.0",
  "description": "Export GitHub secrets & vars to environment variables & specified file",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest --testPathIgnorePatterns test/e2e --watchAll",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest --testPathPatterns='test/e2e/.*\\.test\\.ts'",
    "build": "tsc",
    "package": "ncc build --source-map --license licenses.txt",
    "prepare:release": "rimraf build && pnpm run build && pnpm run package",
    "lint": "eslint .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts)\"",
    "update:all": "corepack up && pnpm update"
  },
  "author": "Jakub Cie≈õlik",
  "license": "MIT",
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912",
  "dependencies": {
    "@actions/core": "^1.11.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.35.0",
    "@jest/globals": "^30.1.2",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/node": "^24.4.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
